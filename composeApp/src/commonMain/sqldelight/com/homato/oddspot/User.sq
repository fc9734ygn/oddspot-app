CREATE TABLE User (
    id TEXT PRIMARY KEY,
    userId TEXT NOT NULL,
    name TEXT NOT NULL,
    imageUrl TEXT,
    email TEXT NOT NULL,
    jwt TEXT NOT NULL
);

insertUser:
INSERT OR REPLACE INTO User(id, userId, name, imageUrl, email, jwt)
VALUES (?,?, ?,?, ?, ?);

selectUser:
SELECT * FROM User;

deleteUser:
DELETE FROM User;

updateUsername:
UPDATE User SET name = ? WHERE id = ?;